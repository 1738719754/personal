<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youmel.auth.mapper.BaseMenuCustomerMapper">
    <select id="selectAllMenu" resultType="com.youmel.auth.pojo.BaseMenu">
        SELECT
          id as id,
          title as title,
          parent_id as parentId,
          href as href,
          icon as icon
        FROM
         base_menu
    </select>
</mapper>